{
  "name": "vue-remote-slides",
  "description": "A Vue.js remote controller for slides",
  "version": "1.0.0",
  "author": "zeratulmdq <colomdq85@hotmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "server": "node ./backend/server.js",
    "go": "concurrently --kill-others \"npm run server\" \"npm run dev\""
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "connect-busboy": "^0.0.2",
    "express": "^4.14.1",
    "pdfjs-dist": "^1.7.308",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "vue": "^2.1.0",
    "vue-resource": "^1.2.1",
    "vue-router": "^2.2.0",
    "vuex": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.22.0",
    "concurrently": "^3.1.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
